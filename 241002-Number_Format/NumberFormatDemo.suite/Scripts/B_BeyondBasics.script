// The NumberFormat can display non-decimal values
set the listFormat to {prefix:"[ ", suffix:" ]", separator:" , "}

set numbers to [0, -12, 98.6, 123.4567, 1/3]

set the numberFormat to "hexadecimal"
put the numberFormat
put numbers
put -

set the numberFormat to "octal"
put the numberFormat
put numbers
put -

set the numberFormat to "binary"
put the numberFormat
put numbers
put -

