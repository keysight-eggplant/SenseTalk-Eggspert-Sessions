(* Iterating over the keys and values in a property list *)

set scores to {
	James:35,
	Dorothy:46,
	Billy:81,
	Frances:63,
	Flora:77
}

repeat with each name in the keys of scores
	put scores.(name) into score
	put !"[[name]] has a score of [[score]]"
end repeat

put "-------------------"

repeat with each [name,score] in scores
	put !"[[name]] has a score of [[score]]"
end repeat
